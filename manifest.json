{
  "name": "Highlight Bear",
  "description": "Highlight Bear is a chrome extension that allows you to highlight text on any pages.",
  "key": "",
  "version": "0.13",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon-16x16.png",
    "32": "icons/icon-32x32.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "options_page": "src/options/options.html",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon-16x16.png",
      "32": "icons/icon-32x32.png",
      "48": "icons/icon-48x48.png",
      "128": "icons/icon-128x128.png"
    },
    "default_title": "Highlight Bear"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content/highlighter.js"
      ],
      "run_at": "document_end"
    }
  ]
}
