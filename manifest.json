{
  "name": "Chrome extension template",
  "description": "This is a simple chrome extension template.",
  "version": "0.3",
  "manifest_version": 3,
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {}
}